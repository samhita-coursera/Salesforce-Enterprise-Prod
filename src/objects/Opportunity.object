    <fields>
        <fullName>Close_Date_Custom__c</fullName>
        <description>First day of the close date quarter</description>
        <externalId>false</externalId>
        <formula>if(year(CloseDate)=2016, 
if(month(CloseDate)=1,CloseDate-day(CloseDate)+1, 
if(month(CloseDate)=2,CloseDate-day(CloseDate)-31+1, 
if(month(CloseDate)=3,CloseDate-day(CloseDate)-29-31+1, 
if(month(CloseDate)=4,CloseDate-day(CloseDate)+1, 
if(month(CloseDate)=5,CloseDate-day(CloseDate)-30+1, 
if(month(CloseDate)=6,CloseDate-day(CloseDate)-31-30+1, 
if(month(CloseDate)=7,CloseDate-day(CloseDate)+1, 
if(month(CloseDate)=8,CloseDate-day(CloseDate)-31+1, 
if(month(CloseDate)=9,CloseDate-day(CloseDate)-31-31+1, 
if(month(CloseDate)=10,CloseDate-day(CloseDate)+1, 
if(month(CloseDate)=11,CloseDate-day(CloseDate)-31+1, 
CloseDate-day(CloseDate)-30-31+1))))))))))), 
if(month(CloseDate)=1,CloseDate-day(CloseDate)+1, 
if(month(CloseDate)=2,CloseDate-day(CloseDate)-31+1, 
if(month(CloseDate)=3,CloseDate-day(CloseDate)-28-31+1, 
if(month(CloseDate)=4,CloseDate-day(CloseDate)+1, 
if(month(CloseDate)=5,CloseDate-day(CloseDate)-30+1, 
if(month(CloseDate)=6,CloseDate-day(CloseDate)-31-30+1, 
if(month(CloseDate)=7,CloseDate-day(CloseDate)+1, 
if(month(CloseDate)=8,CloseDate-day(CloseDate)-31+1, 
if(month(CloseDate)=9,CloseDate-day(CloseDate)-31-31+1, 
if(month(CloseDate)=10,CloseDate-day(CloseDate)+1, 
if(month(CloseDate)=11,CloseDate-day(CloseDate)-31+1, 
CloseDate-day(CloseDate)-30-31+1))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>First day of the close date quarter</inlineHelpText>
        <label>Close Date (Custom)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Close_Date_in_Current_Quarter__c</fullName>
        <description>If the quarter of the close date matches the current quarter, then true; otherwise false</description>
        <externalId>false</externalId>
        <formula>OR(MONTH(TODAY())=MONTH(CloseDate),
AND(OR(MONTH(TODAY())=1,MONTH(TODAY())=2,MONTH(TODAY())=3),OR(MONTH(CloseDate)=1,MONTH(CloseDate)=2,MONTH(CloseDate)=3)),
AND(OR(MONTH(TODAY())=4,MONTH(TODAY())=5,MONTH(TODAY())=6),OR(MONTH(CloseDate)=4,MONTH(CloseDate)=5,MONTH(CloseDate)=6)),
AND(OR(MONTH(TODAY())=7,MONTH(TODAY())=8,MONTH(TODAY())=9),OR(MONTH(CloseDate)=7,MONTH(CloseDate)=8,MONTH(CloseDate)=9)),
AND(OR(MONTH(TODAY())=10,MONTH(TODAY())=11,MONTH(TODAY())=12),OR(MONTH(CloseDate)=10,MONTH(CloseDate)=11,MONTH(CloseDate)=12)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If the quarter of the close date matches the current quarter, then true; otherwise false</inlineHelpText>
        <label>Close Date in Current Quarter</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Close_Date_in_Previous_Quarter_CY__c</fullName>
        <description>True if the close date is in a previous quarter from the same year, otherwise false</description>
        <externalId>false</externalId>
        <formula>OR(
AND(MONTH(TODAY())&gt;=4,MONTH(CloseDate)&lt;4), 
AND(MONTH(TODAY())&gt;=7,MONTH(CloseDate)&lt;7), 
AND(MONTH(TODAY())&gt;=10,MONTH(CloseDate)&lt;10))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>True if the close date is in a previous quarter from the same year, otherwise false</inlineHelpText>
        <label>Close Date in Previous Quarter (CY)</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
