    <validationRules>
        <fullName>ACV_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
( 
Admin_Override__c = False, 
ISCHANGED(StageName), 
NOT(ISPICKVAL(StageName,&quot;Qualified Opportunity&quot;)),
ISBLANK( ACV_Quota_Retirement__c )
)</errorConditionFormula>
        <errorDisplayField>ACV_Quota_Retirement__c</errorDisplayField>
        <errorMessage>Please enter ACV</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(Admin_Override__c),
NOT($Profile.Name = &quot;System Administrator&quot;), 
ISCHANGED(CloseDate),
NOT(ISNEW()),
ISPICKVAL( PRIORVALUE( StageName ) , &quot;Closed Won&quot;)
)</errorConditionFormula>
        <errorMessage>Contact your system administrator to change close date of a closed/won opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
 Admin_Override__c = False, 
ISCHANGED(StageName), 
NOT( ISPICKVAL( PRIORVALUE( StageName ) , &quot;Closed Lost&quot;) ),
NOT ( ISPICKVAL (PRIORVALUE( StageName ), &quot;Closed Lost - No Opportunity&quot;) ),
OR(ISPICKVAL( StageName , &quot;Closed Lost&quot;),ISPICKVAL( StageName , &quot;Closed Lost - No Opportunity&quot;)),
 CloseDate &lt;&gt;  TODAY()
