    <fields>
        <fullName>B2B_Account_Status__c</fullName>
        <description>If any opportunity in the account is won and its contract end date is &gt;=TODAY, then this is automatically updated to Active; otherwise Prospect</description>
        <externalId>false</externalId>
        <inlineHelpText>If any opportunity in the account is won and its contract end date is &gt;=TODAY, then this is automatically updated to Active; otherwise Prospect</inlineHelpText>
        <label>B2B Account Status</label>
        <summaryFilterItems>
            <field>Opportunity.Probability</field>
            <operation>equals</operation>
            <value>100</value>
        </summaryFilterItems>
        <summaryFilterItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Won</value>
        </summaryFilterItems>
        <summaryForeignKey>Opportunity.AccountId</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <trackHistory>false</trackHistory>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>B2B_Acct_Status__c</fullName>
        <description>If the account has an opportunity that is won and its contract end date &gt;= today, then Active; otherwise Prospect</description>
        <externalId>false</externalId>
        <formula>if( AND (B2B_Account_Status__c &gt;0, Number_of_Renewals_Lost__c &lt;1),&quot;Active&quot;, IF(AND(B2B_Account_Status__c &gt;0, Number_of_Renewals_Lost__c &gt;=1),&quot;Churned customer&quot;,&quot;Prospect&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If the account has an opportunity that is won and its contract end date &gt;= today, then Active; otherwise Prospect</inlineHelpText>
        <label>B2B Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
