    <validationRules>
        <fullName>Billing_address_is_required</fullName>
        <active>true</active>
        <description>This rule makes sure that the Billing Country, State and Postal Code are populated on Quote - This is required as it will be used on Quote Templates</description>
        <errorConditionFormula>OR(ISBLANK(BillingCountry),  ISBLANK( BillingState ),  
AND(ISBLANK(BillingCity),ISBLANK(BillingPostalCode))
)</errorConditionFormula>
        <errorMessage>Incomplete Bill To Address. Please enter State, Country and City/Postal Code.</errorMessage>
    </validationRules>
